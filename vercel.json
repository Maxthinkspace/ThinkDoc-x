{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "installCommand": "cd packages/web-app && npm install --legacy-peer-deps",
  "buildCommand": "cd packages/web-app && CI=false npm run build",
  "outputDirectory": "packages/web-app/dist",
  "framework": null,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
